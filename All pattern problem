//EDCB
//EDC
//ED
//E
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 5; j >= i; j--)
        {
            printf("%c", j + 64);
        }
        printf("\n");
    }
}

// *******************************************************************
//ABCDCBA
//ABC CBA
//AB BA
//A  A
#include <stdio.h>
int main()
{
    int i, j;
    char k;
    for (i = 1; i <= 4; i++)
    {
        k = 'A';
        for (j = 1; j <= 7; j++)
        {
            if (j <= 5 - i || j >= 3 + i)
            {
                printf("%c", k);
                j < 4 ? k++ : k--;
            }
            else
            {
                printf(" ");
                if (j == 4)
                    k--;
            }
        }
        printf("\n");
    }
}


// **************************************************************
//EDCBA
//DCBA
//CBA
//BA
//A
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 5; i >= 1; i--)
    {
        for (j = i; j >= 1; j--)
        {
            printf("%c", j + 64);
        }
        printf("\n");
    }
}


// ****************************************************************
//ABCDEFG rotated u star
#include <stdio.h>
int main()
{
    int i, j;
    char k;
    for (i = 1; i <= 4; i++)
    {
        k = 'A';
        for (j = 1; j <= 7; j++)
        {
            if (j <= 5 - i || j >= 3 + i)
                printf("%c", k);
            else
                printf(" ");
            k++;
        }
        printf("\n");
    }
}


