// **********************************************************
// student marks roll Displaying Information
#include <stdio.h>
struct student
{
    char name[50];
    int roll;
    float marks;
} s;

int main()
{
    printf("Enter information:\n");
    printf("Enter name: ");
    fgets(s.name, sizeof(s.name), stdin);

    printf("Enter roll number: ");
    scanf("%d", &s.roll);
    printf("Enter marks: ");
    scanf("%f", &s.marks);

    printf("Displaying Information:\n");
    printf("Name: ");
    printf("%s", s.name);
    printf("Roll number: %d\n", s.roll);
    printf("Marks: %.1f\n", s.marks);

    return 0;
}


// *********************************

//* in star pattern
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 0; i <= 11; i++)
    {
        for (j = 0; j <= 5; j++)
        {
            if (i == 3 && j == 0 || i == 3 && j == 5 || i == 4 && j == 1 || i == 4 && j == 4 || i == 5 && j == 2 || i == 5 && j == 3 || i == 7 && j == 2 || i == 7 && j == 3 || i == 8 && j == 1 || i == 8 && j == 4 || i == 9 && j == 0 || i == 9 && j == 5)
            {
                printf("**");
            }
            else if (i == 6 && j == 2)
            {
                printf(" *");
            }
            else if (i == 6 && j == 3)
            {
                printf("* ");
            }
            else
            {
                printf("  ");
            }
        }
        printf("\n");
    }
}

