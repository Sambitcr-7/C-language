// ****************************************************
//Printing name in Triangle
#include <stdio.h>
#include <string.h>
#include <conio.h>

int main()
{
    char name[] = "Karn Tiwari";
    int len, i, j;

    len = strlen(name);

    for (i = 0; i < len; i++)
    {
        for (j = 0; j <= i; j++)
        {
            printf("%c", name[j]);
        }
        printf("\n");
    }
    getch();
    return 0;
}

