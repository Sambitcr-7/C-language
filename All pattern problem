            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}

// *************************************
// p
// **
// PPP
// ****
// PPPPP
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= i; j++)
        {
            if (i % 2 == 0)
            {
                printf("*");
            }
            else
            {
                printf("P");
            }
        }
        printf("\n");
    }
    return 0;
}

