// *****************************************************************
//0
//01
//024
//0369
//0481216
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 5; i++)
    {
        k = 0;
        for (j = 1; j <= 5; j++)
        {
            if (j <= i)
            {
                printf("%d", k);
                k = k + i - 1;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}


// *************************************************************************
//1
//62
//1073
//131184
//15141295
// input 6 or any no
#include <stdio.h>
int main()
{
    int i, j, row, p, k = 1;
    printf("Enter number of rows :");
    scanf("%d", &row);
    for (i = 1; i <= row; i++)
    {
        p = k;
        for (j = 1; j <= i; j++)
        {
            printf("%d ", p);
            p = p - (row - i + j);
        }
        printf("\n");
        k = k + 1 + row - i;
    }
}


// ***********************************************************************
//6543210
// 543210
//43210
//3210
//210
//10
//0
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 7; i++)
    {
        k = 7 - i;
        for (j = 1; j <= 7; j++)
        {
            if (j <= 8 - i)
            {
                printf("%d ", k);
                k--;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}

// ********************************************************************
// loop star* in any no entery
#include <stdio.h>
int main()
{
    int i, k, flag = 1, row;
    printf("Enter number of lines :");
    scanf("%d", &row);
    for (i = 1, k = 0; i <= 2 * row - 1; i++)
    {
        if (i < row - k)
            printf(" ");
        else
        {
            if (flag)
                printf("* ");
            else
                printf("");
            flag = 1 - flag;
        }
        if (i == row + k)
        {
            k++;
            printf("\n");
            if (i == 2 * row - 1)
                break;
            i = 0;
            flag = 1;
        }
    }
}

