// ******************************************************
//*********
//*       *
//*       *
//*********
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= 5; j++)
        {
            if (i == 1 || i == 5 || j == 1 || j == 5)
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }
}


// ************************************************************
//*
//**
//*  *
//*    *
//*      *
//**********
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= 5; j++)
        {
            if (j == 1 || i == 5 || j == i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}

// *******************************************
//roteded u star
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 4; i++)
    {
        for (j = 1; j <= 7; j++)
        {
            if (j <= 5 - i || j >= 3 + i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}

// ********************************************************************
// O star
#include <stdio.h>
int main()
{
    int i, j, k = 0;
    for (i = 1; i <= 9; i++)
    {
        i <= 5 ? k++ : k--;
        for (j = 1; j <= 9; j++)
        {
            if (j <= 6 - k || j >= 4 + k)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}

// *************************************************************************
// x line
//and x star
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 7; i++)
    {
        for (j = 1; j <= 7; j++)
        {
            if (j == i || j == 8 - i)
            {
                if (i == 8 - j)
                    printf("/");
                else
                    printf("\\");
            }
            else
                printf("*");
        }
        printf("\n");
    }
}


// *********************************************************************
// X star
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= 5; j++)
        {
            if (j == i || j == 6 - i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}

// ***********************************************************8
//     1
//   A  B
// 1  2  3
//A B C  D
#include <stdio.h>
int main()
{
    int i, j, k, line, num;
    char alpha;
    printf("Enter number of lines :");
    scanf("%d", &line);
    for (i = 1; i <= line; i++)
    {
        k = 1;
        num = 1;
        alpha = 'A';
        for (j = 1; j <= 2 * line - 1; j++)
        {
            if (j >= line + 1 - i && j <= line - 1 + i)
            {
                if (k)
                {
                    if (i % 2 == 1)
                        printf("%d", num++);
                    else
                        printf("%c", alpha++);
                }
                else
                    printf(" ");
                k = 1 - k;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}


// **********************************************************************
//              *
//               **
//             ***
//                **
//    1
//    1
//    1
//*
//**
//****
//**
//*  input is 6 or any no
#include <stdio.h>
int main()
{
    int n, i, j, k;
    printf("Enter value of n=");
    scanf("%d", &n);

    for (i = 1; i <= 3 * n; i++)
    {
        for (j = 1; j <= 2 * n - 1; j++)
        {
            if (i <= n)
            {
                if (j >= 2 * n - i)
                    printf("*");
                else
                    printf(" ");
            }
            else if (i <= 2 * n)
            {
                k = 0;
                if (j == n)
                {
                    k = 1;
                    printf("|");
                }
                if (j <= (i - n - 1))
                {
                    k = 1;
                    printf("*");
                }
                if (j >= i)
                {
                    k = 1;
                    printf("*");
                }
                if (k == 0)
                    printf(" ");
            }
            else if (i <= 3 * n)
            {
                if (j <= 3 * n + 1 - i)
                    printf("*");
                else
                    printf(" ");
            }
        }
        printf("\n");
    }
}

// ***************************************************************
//    A
//   CB
//  FED
// JIHG
//ONMLK
#include <stdio.h>
int main()
{
    int i, j;
    char k = 64;
    for (i = 1; i <= 5; i++)
    {
        k = k + 2 * (i - 1) + 1;
        for (j = 1; j <= 5; j++)
        {
            if (j >= 6 - i)
                printf("%c", k--);
            else
                printf(" ");
        }
        printf("\n");
    }
}


// **************************************************************
//1
//3*2
//4*5*6
//10*9*8*7
//11*12*13*14*15
#include <stdio.h>
int main()
{
    int i, j, flag, k = 0, p;
    for (i = 1; i <= 5; i++)
    {
        if (i % 2 == 1)
            k = k + 1;
        else
        {
            k--;
            k = k + i;
        }
        flag = 1;
        p = k;
        for (j = 1; j <= 9; j++)
        {
            if (j <= 2 * i - 1)
            {
                if (flag == 1)
                {
                    printf("%d", p);
                    if (i % 2)
                    {
                        p++;
                        k++;
                    }
                    else
                        p--;
                }
                else
                    printf("*");
                flag = 1 - flag;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}


// *****************************************************************
//star Pattern -25
#include <stdio.h>
int main()
{
    int i, j, k = 1, n = 11, m = 0;
    for (i = 1; i <= 21; i++)
    {
        i <= n ? m++ : m--;
        k = m - 1;
        for (j = 1; j <= 21; j++)
        {
            if (j >= n + 1 - m && j <= n - 1 + m)

                printf(" %d", j < n ? k++ % 10 : k-- % 10);

            else
                printf("  ");
        }
        printf("\n");
    }
}

// *************************************************************8
//Square in square
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 7; i++)
    {
        for (j = 1; j <= 7; j++)
        {
            if ((i == 1 || i == 7 || j == 1 || j == 7) || (i >= 3 && i <= 5 && j >= 3 && j <= 5) && (i == 3 || i == 5 || j == 3 || j == 5))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}


// *******************************************************
//1
//12A
//123AB
//123ABC
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 4; i++)
    {
        k = 49;
        for (j = 1; j <= 7; j++)
        {
            if (j >= -i && j <= 3 + i)
            {
                printf("%c", k);
                k++;
                if (j == 4)
                    k = 65;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}


// *************************************************************
//A1
//AB12
//ABC123
//ABCD1234
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 4; i++)
    {
        k = 'A';
        for (j = 1; j <= 8; j++)
        {
            if (j >= 5 - i && j <= 4 + i)
            {
                if (j == 5)
                    k = '1';
                printf("%c", k);
                k++;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}

// *************************************************
//A
//ABA
//ABCBA
//ABCDBA
#include <stdio.h>
int main()
{
    int i, j;
    char k;
    for (i = 1; i <= 4; i++)
    {
        k = 'A';
        for (j = 1; j <= 7; j++)
        {
            if (j >= 5 - i && j <= 3 + i)
            {
                printf("%c", k);
                j < 4 ? k++ : k--;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}

// ***************************************************************
// 1 in center
// 23
// 345
// 678910
#include <stdio.h>
int main()
{
    int num, r, c, sp, i = 1;
    printf("Enter any number : ");
    scanf("%d", &num);
    for (r = 1; r <= num; r++)
    {
        for (sp = 1; sp <= num - r; sp++)
            printf(" ");
        for (c = 1; c <= r; c++, i++)
            printf("%d ", i);
        printf("\n");
    }
    return 0;
}

// ************************************************************

// 1  2  3  4  5
// 16          6
// 15          7
// 14          8
// 13 12 11 10 9
int main()
{
    int i, j, k = 6, l = 13, m = 16;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= 5; j++)
        {
            if (i == 1)
                printf("%-3d", j);
            else if (j == 5)
                printf("%-3d", k++);
            else if (i == 5)
                printf("%-3d", l--);
            else if (j == 1)
                printf("%-3d", m--);
            else
                printf("   ");
        }
        printf("\n");
    }
    return 0;
}


// ************************************************************************
// ABCDEFGFEDCBA
// ABCDEF  EDCBA
// ABCDE     DCBA
// ABCD         CBA
// ABC             BA
// AB                 A
// A

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, j;

    for (i = 1; i <= 7; i++)
    {

        for (j = 1; j <= 8 - i; j++)
        {
            printf("%c", (64 + j));
        }

        for (int m = 1; m <= i - 1; m++)
        {
            printf(" ");
        }
        for (int n = 1; n <= i - 1; n++)
        {
            printf(" ");
        }

        for (int k = 7 - i; k >= 1; k--)
        {
            printf("%c", (64 + k));
        }

        printf("\n");
    }

    return 0;
}

// ***********************************************************
// ABCDEFGFEDCBA
// ABCDEF FEDCBA
// ABCDE   EDCBA
// ABCD     DCBA
// ABC       CBA
// AB         BA
// A           A
#include <stdio.h>
#include <conio.h>
int main()
{
    int i, j, m, k = 0, l, p = 0, o, a, b = 0, c = 0;
    m = 'G';
    o = 'F';
    for (j = 1; j <= 7; j++)
    {
        for (i = 'A'; i <= (m - k); i++)
        {
            printf("%c", i);
        }
        for (a = 1; a < b; a++)
        {
            printf(" ");
        }
        for (l = (o - p); l >= 'A'; l--)
        {
            printf("%c", l);
        }
        p = p + c;
        b = b + 2;
        k = k + 1;
        c = 1;
        printf("\n");
    }
    getch();
}

// ***************************************************
//*
//**
//***
//*****
// using printf
#include <stdio.h>
int main()
{
    int i, j, n;
    printf("Enter number of rows: ");
    scanf("%d", &n);
    for (i = 1; i <= n; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf("* ");
        }
        //Ending line after each row
        printf("\n");
    }
    return 0;
}

// ****************************************************************
// Hollow triangle in Right angle triangle
#include <stdio.h>
int main()
{
    int x = 0, y = 0;
    int rows = 0;
    printf("Enter the number of rows = ");
    scanf("%u", &rows);
    for (x = 1; x <= rows; ++x)
    {
        for (y = 1; y <= x; ++y)
        {
            if ((y == 1) || (y == x) || (x == rows))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}

// ******************************************************
//Heart
#include <stdio.h>
int main()
{
    int i, j, rows;
    printf("Enter the number of rows\n");
    scanf("%d", &rows);
    for (i = rows / 2; i <= rows; i += 2)
    {
        for (j = 1; j < rows - i; j += 2)
        {
            printf(" ");
        }
        for (j = 1; j <= i; j++)
        {
            printf("*");
        }
        for (j = 1; j <= rows - i; j++)
        {
            printf(" ");
        }
        for (j = 1; j <= i; j++)
        {
            printf("*");
        }
        printf("\n");
    }
    for (i = rows; i >= 1; i--)
    {
        for (j = i; j < rows; j++)
        {
            printf(" ");
        }
        for (j = 1; j <= (i * 2) - 1; j++)
        {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}

// ****************************************************
//hollow heart
#include <stdio.h>
int main()
{

    int n, row, col;
    n = 6;
    for (row = 0; row < n; row++)
    {
        for (col = 0; col < n + 1; col++)
            if ((row == 0 && col % 3 != 0) || (row == 1 && col % 3 == 0) || (row - col == 2) || (row + col == 8))
                printf("*");
            else
                printf(" ");

        printf("\n");
    }
    return 0;
}


// ************************************************************

// X pattern in star
#include <stdio.h>
int main()
{

    int i, j, size;

    printf("Enter size : ");
    scanf("%d", &size);

    for (i = 0; i < size; i++)
    {
        for (j = 0; j < size; j++)
        {

            if (i == j || j == (size - i - 1))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}

// ***********************************************************
// Right angle Face Emoji in pattern
#include <stdio.h>
int main()
{
    int row, col, no;
    printf("Enter Number of rows :");
    scanf("%d", &no);
    for (row = 1; row <= no; row++)
    {
        for (col = 1; col <= row; col++)
        {
            printf(" %C ", 1);
            printf("\n");
        }
    }
    return 0;
}


// *********************************************************
// Face Emoji in pattern
#include <stdio.h>
int main()
{
    int row, col, norow, sps;
    printf("Enter number of rows :");
    scanf("%d", &norow);
    for (row = 1; row <= norow; row++)
    {
        for (sps = 1; sps <= norow - row; sps++)
        {
            printf(" ");
        }
        for (col = 1; col <= row; col++)
        {
            printf(" %c", 1);
            printf("\n");
        }
    }
    return 0;
}

