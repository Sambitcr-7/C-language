int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf("%d", j);
        }
        printf("\n");
    }
}

// *******************************************************
//1
//121
//12321
//1234321
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 4; i++)
    {
        k = 1;
        for (j = 1; j <= 7; j++)
        {
            if (j >= 5 - i && j <= 3 + i)
            {
                printf("%d", k);
                j < 4 ? k++ : k--;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}

// ***************************************************
//1
//10
//101
//1010
//10101
#include <stdio.h>
int main()
{
    int i, j;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf("%d", j % 2);
        }
        printf("\n");
    }
}

// *****************************************************
//1
//01
//010
//1010
//10101
#include <stdio.h>
int main()
{
    int i, j, k = 1;
    for (i = 1; i <= 5; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf("%d ", (k++) % 2);
        }
        printf("\n");
    }
}

