// **********************************************************8
//1 left side
//12
//123
//1234
//12345
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 5; i++)
    {
        for (k = 5; k >= i; k--)
            printf(" ");
        for (j = 1; j <= i; j++)
        {
            printf("%d", j);
        }
        printf("\n");
    }
}


// *****************************************************************
//1
//212
//32123
//43211234 center
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 4; i++)
    {
        k = i;
        for (j = 1; j <= 7; j++)
        {
            if (j >= 5 - i && j <= 3 + i)
            {
                printf("%d", k);
                j < 4 ? k-- : k++;
            }
            else
                printf(" ");
        }
        printf("\n");
    }
}

// **********************************************************************
//1 center side
//12
//123
//1234
//12345
#include <stdio.h>
int main()
{
    int i, j, k;
    for (i = 1; i <= 5; i++)
    {
        for (k = 5; k >= i; k--)
            printf(" ");
        for (j = 1; j <= i; j++)
        {
            printf("%d ", j);
        }
        printf("\n");
    }
}


// *******************************************************************************
//1 in center
//12
//123
//12345
#include <stdio.h>
int main()
{
    int i, j, k;
    k = 1;
    for (i = 1; i <= 5; i += 2)
    {
        for (j = 5; j >= 1; j--)
        {
            if (j > i)
                printf(" ");
            else
                printf("%d ", k++);
        }
        printf("\n");
    }
    return 0;
}

