// *********************************************************
// 1 to 5
#include <stdio.h>
void main()
{
    int j;
    for (j = 1; j <= 5; j++)
    {
        printf("%d ", j);
    }
}

// *****************************************************************
//5 to 1
#include <stdio.h>
void main()
{
    int j;
    for (j = 5; j >= 1; j--)
    {
        printf("%d ", j);
    }
}

// *********************************************************************
// 12345X4
#include <stdio.h>
void main()
{
    int i, j;
    for (i = 1; i <= 4; i++)
    {
        for (j = 1; j <= 5; j++)
        {
            printf("%d ", j);
        }
    }
}

// ************************************************************
